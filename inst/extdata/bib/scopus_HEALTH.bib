@BOOK{Tsolaki201619,
	author = {Tsolaki, Magda},
	title = {Clinical symptomatology of pituitary adenomas: From a neuropsychiatric point of view},
	year = {2016},
	journal = {Pituitary Adenoma: Pathophysiology, Diagnosis and Treatment Options},
	pages = {19 – 92},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85019955962&partnerID=40&md5=6796116d3e7c4132e9c639ada6eabec8},
	affiliations = {3rd Department of Neurology, School of Health Sciences, Medical Faculty, Aristotle University of Thessaloniki, Despere Str. 3, Thessaloniki, 54621, Greece},
	abstract = {Pituitary adenomas have been associated with multiple co-morbidities, including neuropsychiatric disorders. The overall perceived prevalence and severity of cognitive dysfunction were the highest among Non-Functional Pituitary Adenomas (NFPA) groups, particularly in the areas of mental agility, verbal recall, and memory recall. The main psychiatric symptom of hypopituitarism is a combination of dementia and delirium. However, the most common neurologic symptoms are headaches and visual changes. Thirty-three to seventy-two percent of pituitary adenomas, independently of their dimensions, induce headache. Non-functional pituitary adenomas tend to affect visual acuity more than adenomas with other pathological diagnoses, which produce either adrenocorticotropic hormone or prolactin. Visual Field defects occurred in 95% of patients with non-functioning pituitary macroadenoma. The above neurologic symptoms are more common in nonfunctioning adenomas but also in gonadotroph adenomas because these tumors do not secrete sufficient hormones to cause endocrine-type symptoms. ACTH-secreting adenomas causing Cushing Disease cause significant morbidity. Carpal tunnel syndrome, obstructive sleeping apnea and headache are typical neurological features in somatotroph adenomas. Pain appears to be a frequent problem in pituitary disease. The data suggest that pain should be integrated in the diagnostic and therapeutic work-up of patients with pituitary disease in order to treat them appropriately and improve their Quality of Life (QoL). Meningitis is an unusual complication of untreated invasive pituitary adenomas and represents the initial symptom leading to diagnosis of a macroadenoma in exceptional cases. If the adenoma expands laterally into the cavernous sinus, it has the potential to affect the cranial nerves housed there, including cranial nerves III (oculomotor), IV (trochlear), VI (abducens), thus causing diplopia. Other neurologic presentations, such as cerebrospinal fluid rhinorrhea, seizures, and pituitary apoplexy, can occur but are uncommon. Other uncommon symptoms are hoarse voice, acute respiratory distress due to unilateral larynx paralysis and myopathy. Prolactin-secreting pituitary adenomas (PRL-omas) are well-differentiated endocrine tumors expressing D2 dopamine receptor (DRD2). DRD2 polymorphisms correlate with psychiatric disorders, in particular alcoholism and schizophrenia. Compared with healthy controls, acromegalic patients described themselves as distinctly more harm avoidant and neurotic and presented themselves with high social conformity. However, specific for patients with GH-producing adenomas was an even more reduced novelty-seeking behavior, especially in terms of lower impulsiveness, compared with patients with nonfunctioning pituitary adenomas. Patients with pituitary adenomas show a distinct pattern of increased anxiety-related personality traits compared with the general population, potentially as a result of the pituitary lesion and/or associated hormonal dysregulations and comorbidities. In conclusion many neurological and psychiatric symptoms can accompany pituitary adenomas. © 2016 by Nova Science Publishers, Inc. All rights reserved.},
	author_keywords = {Functional and non-functional pituitary adenomas; Psychiatric and neurological symptoms},
	correspondence_address = {M. Tsolaki; 3rd Department of Neurology, School of Health Sciences, Medical Faculty, Aristotle University of Thessaloniki, Thessaloniki, Despere Str. 3, 54621, Greece; email: tsolakim1@gmail.com},
