@BOOK{Davis2005425,
	author = {Davis, Stephen D. and Ewers, Frank W. and Pratt, R.Brandon and Brown, Pamela L. and Bowen, Timothy J.},
	title = {Interactive effects of freezing and drought on long distance transport: A case study of chaparral shrubs of california},
	year = {2005},
	journal = {Vascular Transport in Plants},
	pages = {425 – 435},
	doi = {10.1016/B978-012088457-5/50022-8},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-33845703195&doi=10.1016%2fB978-012088457-5%2f50022-8&partnerID=40&md5=589c4ee75dcbcdb98273ccacc5137ef9},
	affiliations = {Natural Science Division, Pepperdine University, Rhodes University, Malibu, CA, South Africa; Department of Plant Biology, Michigan State University, East Lansing, 48824-1312, MI, United States; Natural Science Division, Pepperdine University, Malibu, 90263-4321, CA, United States},
	abstract = {Chaparral species are evergreen and retain metabolically functional leaves throughout the year, requiring uninterrupted supplies of water to remain hydrated. Water stress-induced or freeze-thaw-induced embolism of xylem water conduits represents a permanent loss of hydraulic capacity, as there is no evidence of reversal mechanism in adult shrubs. Assuming that resistance to freezing- or drought-induced injury incurs some cost, one might surmise that leaves should be about equal in susceptibility to injury by water stress and freezing as xylem conduits are to water stress and freezing-caused embolism. In C. crassifolius, stem xylem is more susceptible to water stress than are leaves (whole branchlet dieback with no recovery), whereas in the chaparral shrub Quercus berberidifolia leaves are more susceptible than stem xylem (entire leaf canopy dies in response to severe drought and remains on a shrub until new leaf emergence on stems with winter rains). In R. ovata, stem xylem is more susceptible to freeze-caused dysfunction than leaves; however, in a closely related species, M. laurina, leaves and stems are equally susceptible to freezing injury. Thus the assumption of equal susceptibility to water stress and freezing between leaves and stem xylem does not always hold. Other survival traits may compensate for imbalances in the susceptibility of leaves and stem xylem, such as the high resistance to water loss in leaves of R. ovata that allows leaves to persist in a partially hydrated state until the vascular cambium produces new xylem. © 2005 Elsevier Inc. All rights reserved.},
	publisher = {Elsevier},
	isbn = {978-012088457-5},
	language = {English},
	abbrev_source_title = {Vascular Transport in Plants},
	type = {Book chapter},
	publication_stage = {Final},
