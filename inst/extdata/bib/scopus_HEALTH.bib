	keywords = {Animals; Biological Evolution; Eukaryota; Eukaryotic Cells; Mitochondria; Phagocytosis; Phylogeny; Symbiosis; animal; eukaryote; eukaryotic cell; evolution; genetics; mitochondrion; phagocytosis; phylogeny; physiology; symbiosis},
