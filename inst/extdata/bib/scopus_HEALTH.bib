}@ARTICLE{Khaltabadi Farahani202241,
	author = {Khaltabadi Farahani, A.H. and Mohammadi, H. and Moradi, M.H. and Ghasemi, H.A. and Hajkhodadadi, I.},
	title = {Genomic-wide association study for egg weight-related traits in Rhode Island Red breed using Bayesian methods},
	year = {2022},
	journal = {Animal Production Research},
	volume = {11},
