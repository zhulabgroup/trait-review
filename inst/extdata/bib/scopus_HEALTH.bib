@BOOK{Srinivas2017236,
	author = {Srinivas, Krishna Ravi},
	title = {The regulatory regime for genetically modified crops in india},
	year = {2017},
	journal = {Genetically Modified Organisms in Developing Countries: Risk Analysis and Governance},
	pages = {236 – 246},
	doi = {10.1017/9781316585269.021},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85047971423&doi=10.1017%2f9781316585269.021&partnerID=40&md5=aa59c878c265b27085f720c0221eb847},
	affiliations = {Research and Information System for Developing Countries (RIS), New Delhi, India},
	abstract = {India was one of the few developing countries to initiate policies for the development of biotechnology even when the biotechnology revolution had not developed fully. Initially the Government of India established in 1983 a Board on Biotechnology, which was converted into the Department of Biotechnology (DBT) in 1985. Since then biotechnology has been supported by the Government of India through various programmes, including efforts in capacity building and human resources development. The opening up of the economy and liberalisation since 1991 provided many opportunities for the biotechnology sector in India. India became one of the favourite destinations for biotechnology research and development (R&D) and at the same time attracted foreign direct investments. With India becoming a global player in information technology and enabled services, bioinformatics and bioservices became important sectors in biotechnology. According to a recent estimate, India's bioeconomy is valued at about US$35 billion and the share of agribiotech is US$9 billion (ABLE, 2016). The DBT published its latest biotechnology strategy in December 2015. Many state governments have their own policies and programmes to promote biotechnology. Thus the policy milieu in India for biotechnology has been favourable and, with the increased emphasis on ‘Make in India’ and new opportunities in sectors like biopharmaceuticals and post-genome life sciences, governments have become all the more supportive of biotechnology. Regarding GM crops though, Bt (Bacillus thuringiensis) cotton is the only crop that has been allowed for commercialisation. Other GM crops in the pipeline and their respective traits are very important for India to meet its needs in agriculture when threats like climate change, constraints on using water resources and soil degradation loom large. Among scientists in India there is almost a consensus that GM crops have a key role to play and that they pose no significant risks to the environment and human health (Giri and Tyagi, 2016). While the policies for promoting and incentivising biotechnology are important, equally important is a biotechnology regulatory regime that would ensure that benefits from biotechnology are not achieved at the cost of environmental safety, human health and conservation of biodiversity. © Cambridge University Press 2017.},
	publisher = {Cambridge University Press},
	isbn = {978-131658526-9; 978-110715191-8},
	language = {English},
	abbrev_source_title = {Genetically Modified Organisms in Developing Ctries.: Risk Analysis and Gov.},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 1}
}

