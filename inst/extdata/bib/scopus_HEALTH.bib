@BOOK{Walsh2019311,
	author = {Walsh, John J. and Lenes, J.M.},
	title = {Efficacies of imprudent top-down and time variant bottom-up controls over 1965-2011 as computed from a validated model of 35 state variables},
	year = {2019},
	journal = {Wind-Borne Illness from Coastal Seas: Present and Future Consequences of Toxic Marine Aerosols},
	pages = {311 – 346},
	doi = {10.1016/B978-0-12-812131-3.00004-5},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081336022&doi=10.1016%2fB978-0-12-812131-3.00004-5&partnerID=40&md5=9e93e5b2b34f03157e887e8a7667cb36},
	affiliations = {University of South Florida, United States; Little Meadows Early Childhood Center, New York, United States},
	abstract = {A revised two-dimensional ecosystem model of 35 state variables, set within the De Soto Canyon ecotone of the Northern Gulf of Mexico and driven by both three-dimensional flow fields from uncoupled circulation models and observed accumulations of zooplankton herbivores, is explored here in terms of the daily (1) food web, (2) public health, and (3) sedimentary consequences of oil-, nutrient-, and overfishing-induced transitions of dominant particle transports to the seafloor: from changing vectors of copepod fecal pellets to those of bacterial-modulated marine snow formations and lysed harmful algal bloom (HAB) toxicities over the annual periods of 1965-2011. Recently spilled petrochemicals were found to minimally impact already decimated zooplankton populations. They facilitated instead formation of marine snow macroaggregates. These recent oil effects just exacerbated other consequences of prior overfishing in the absence of major eutrophication along the eastern side of the ecotone on the WFS (West Florida Shelf). East of that ecotone, overly optimistic removals of piscivore fish stocks over the last half century had already caused a trophic cascade, with ~95% of WFS marine pelagic herbivore losses occurring by 2010, before the Deepwater Horizon (DWH) blowout. By contrast, west of De Soto Canyon, copepods on the eutrophic Louisiana shelf were less impacted by overfishing. Yet during simulated descent of modeled marine snow to the ~1200 m isobath of the upper slope, ~90% of the particulate import to the benthos was now mainly clay minerals of Mississippi River origin along the ecotone, scavenged by aggregates from the water column. The model results here replicated concurrent time series of: (1) annual sediment accumulations, measured at the seafloor; (2) bimonthly onshore supplies of shipboard and current-meter sampled in situ nutrient fluxes, due to upwelling forced remotely by the Loop Current; and (3) near-surface weekly phytoplankton changes of competing functional groups of microalgae, seen by satellite and cell counts. Because of such minimal grazing stresses, after the most recent DWH oil reductions to ~50% of the remaining WFS copepods, their decreased herbivory amounted to only an 8% fecal pellet contribution to computed particle fluxes, settling to the model’s sea bottom. By contrast, 12% of the biotic particle exports, entrained within marine snow, resulted from increased allocthonous loadings of Mississippi River nutrients, while 50% of the ungrazed sinking phytoplankton were fueled by decadal anomalies of increased nutrient supplies from upwelling by the Loop Current. Finally, of the exiting marine snow phytodetritus from the 2010 to 2011 water columns, 30% were due to autocthonous nutrients of the slope water column. Yet, in contrast to the summer-fall oligotrophic HAB accumulations of as much as ~150 umol C L-1 of Karenia brevis, the other eutrophic, sunlit dinoflagellate competitor Cochlodinum sp. behaved instead like the diatom Skeletonema costatum but with winter model blooms of just 3-5 umol C L-1 of this second dinoflagellate simulated by January 2002 and 2011. Moreover, both these simulated and observed stocks of Cochlodinum sp. during 2010-11 on the WFS were ~fivefold less than those found in the adjacent estuaries during March 2011. Such model results indicated that (4) this eutrophic dinoflagellate, with twofold greater required half-saturation constants for phosphorus uptake, was indeed a poor competitor on the WFS against K. brevis, adapted instead there for oligotrophic habitats; and (5) the WFS had not yet been eutrophied, seaward of the mouths of the estuaries, despite popular convictions among some proponents of increased nutrient loadings to Florida’s coastal waters. © 2019 Elsevier Inc. All rights reserved.},
	author_keywords = {Blowout; Carbon budget; Cascade; Circulation model; Deepwater horizon; Oil spill},
	publisher = {Elsevier},
	isbn = {978-012812131-3; 978-012812132-0},
	language = {English},
