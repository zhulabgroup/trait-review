@ARTICLE{Bergmann201790,
	author = {Bergmann, Shana and Schwarzer, Angela and Wilutzky, Katharina and Louton, Helen and Bachmeier, Josef and Schmidt, Paul and Erhard, Michael and Rauch, Elke},
	title = {Behavior as welfare indicator for the rearing of broilers in an enriched husbandry environment—A field study},
	year = {2017},
