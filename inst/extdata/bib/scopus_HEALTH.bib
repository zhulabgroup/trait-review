@BOOK{Gates2014373,
	author = {Gates, C. Cormack},
	title = {What is a wild bison? A case study of plains bison conservation in Canada},
	year = {2014},
	journal = {Ecology, Evolution and Behaviour of Wild Cattle: Implications for Conservation},
	pages = {373 – 384},
	doi = {10.1017/CBO9781139568098.023},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84953219955&doi=10.1017%2fCBO9781139568098.023&partnerID=40&md5=bfc14fb04f2df1965f783c750ff615cb},
	affiliations = {Faculty of Environmental Design, University of Calgary, 2500 University Drive NW, Calgary, AB, Canada},
	abstract = {Globally, the conservation and restoration of viable populations of wild cattle species faces numerous common challenges. Among them are over-exploitation, habitat loss, population decline, hybridization and domestication. Domestication is prevalent among wild cattle species. As a state, it is the antithesis of wildness. As a process, it is driven by several factors, including selective breeding in captivity to propagate traits desirable for the owner and the market (Diamond 2002), and more subtly under captivity through non-intentional mechanisms (Theodorou & Couvet 2004; O’Regan & Kitchener 2005). Human exploitation of free-ranging animals can also induce changes in heritable traits (Darimont et al. 2009). Once lost from the wild or substantially altered by these forces, re-creating a wild species from domestic stock through selective breeding in captivity may involve heroic efforts to produce animals with external morphology similar to an original wild-type, but cannot restore traits previously shaped by natural selection. An example is Hellebrun cattle bred by the Heck brothers in Germany during the 1920s and 1930s in an attempt to ‘re-create’ the extinct aurochs (Bos primigenus) by selectively crossing domestic cattle breeds (Heck 1934; cited by van Vuure 2002; see also Chapter 15). Historically, the American plains bison (Bison bison bison) was perhaps the most abundant wild cattle species in the world, numbering in excess of 30 million (Shaw 1995). Numerous authors have chronicled its near extinction following European colonization and market hunting in the second half of the nineteenth century (Roe 1970; Isenberg 2000). Despite conservation efforts, today there are fewer than 20 000 plains bison in herds managed primarily for conservation, and >96% of plains bison (~400 000) exist under private captive ownership managed as livestock, primarily for commercial purposes (Gates et al. 2010; see also Chapter 9). Even including commercial herds, today American bison occupy less than 1% of their historical range (Sanderson et al. 2008). Despite its numerical abundance, Freese et al. (2007) declared that the plains bison is ‘for all practical purposes ecologically extinct across its former range’. Interest in restoring bison as wildlife and using them as an agent for restoring ecological services has increased during the last decade (Sanderson et al. 2008), yet there has been limited consideration of criteria by which bison can be considered wild in the sense of foundational evolutionary processes. © Cambridge University Press 2014.},
	publisher = {Cambridge University Press},
	isbn = {978-113956809-8; 978-110703664-2},
	language = {English},
	abbrev_source_title = {Ecology, Evolution and Behaviour of Wild Cattle: Implications for Conservation},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}

