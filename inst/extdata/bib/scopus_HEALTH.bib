@ARTICLE{Nazari202217,
	author = {Nazari, S. and Azizi, A. and Kiani, A. and Sharifi, A.},
	title = {Effect of substituting different levels of Camellina sativa meal instead of soybean meal on performance, rumen fermentation parameters, blood metabolites, and feeding behavior of fattening lambs},
	year = {2022},
	journal = {Animal Production Research},
	volume = {11},
	number = {2},
	pages = {17 – 30},
