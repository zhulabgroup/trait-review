@BOOK{Vartanian201713,
	author = {Vartanian, A. and Golubewa, I. and Shprakh, Z.},
	title = {Is vasculogenic mimicry a hallmark of an aggressive tumor?},
	year = {2017},
	journal = {Horizons in Cancer Research},
	volume = {65},
	pages = {13 – 33},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85020323208&partnerID=40&md5=042c1a0e8e10aa4cbb2dc65b9f057460},
	affiliations = {Department of Experimental Diagnosis and Therapy of Tumors, N. N. Blokhin Russian Cancer Research Center, Moscow, Russian Federation},
	abstract = {Cancer development is a multistep process which results in the acquisition of a number of hallmark traits that are common to all cancers, namely sustained proliferative signaling, insensitivity to growth suppressors, resistance to apoptosis, replicative immortality, induction of angiogenesis, activation of invasion and metastasis, reprogrammed energy metabolism, and ability to evade immune control (Hanahan and Weinberg 2011). At the end of 1999, the term of vasculogenic mimicry (VM) was introduced to describe the unique ability of highly aggressive tumor cells to express a multipotent, stem cell-like phenotype and to form vessel-like networks without participation of endothelial cells. VM serves as an alternative mechanism through which tumor cells can obtain nutrients to survive, especially in the less vascularized areas of the tumor. VM has been observed in carcinomas and sarcomas, and its occurrence has been repeatedly demonstrated to have an exceptionally tight association with metastatic cancer-related mortality. Of special significance is the resistance of tumor cell VM to angiogenesis inhibitors, suggesting that VM is a way for tumors to develop insensitivity to this class of anticancer drugs. In this review, we discuss the clinical significance of VM. We discuss the experimental evidence that VM blockade in experimental lung, breast, colon, cervical cancer and melanoma mouse models can reduce tumor growth rate by 80-90%. We also report that the selective low molecular VM inhibitor, LCS1269, restores the sensitivity of resistant melanoma cells to DNA-damaging agents. Finally, we focus on education of non-cancer cells towards VM by aggressive melanoma. In light of these observations, we suggest that VM might be considered as yet another hallmark of aggressive tumor. © 2017 Nova Science Publishers, Inc.},
	correspondence_address = {A. Vartanian; Blokhin Russian Cancer Research Center, Moscow, Kashirskoye shosse 24, 115478, Russian Federation; email: zhivotov57@mail.ru},
	publisher = {Nova Science Publishers, Inc.},
	isbn = {978-153610954-2; 978-153610944-3},
	language = {English},
	abbrev_source_title = {Horiz. in Cancer Res.},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 4}
}

@ARTICLE{Kashtanov20161001,
	author = {Kashtanov, S.N. and Sulimova, G.E. and Shevyrkov, V.L. and Svishcheva, G.R.},
	title = {Breeding of the Russian sable: Stages of industrial domestication and genetic variability},
	year = {2016},
	journal = {Genetika},
	volume = {52},
	number = {9},
	pages = {1001 – 1011},
	doi = {10.7868/s0016675816090071},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85041780090&doi=10.7868%2fs0016675816090071&partnerID=40&md5=f435d084d47ae9e5e951620cd79bfa45},
	abstract = {Creating farms for sable breeding was associated with the commercial destruction of natural populations and, consequently, the overall decline in the species number. The gene pool of the first farm-bred sable population in Russia, established in the vicinity of Moscow (“Pushkinskiy” fur farm), was formed by crossing of animals removed from nine natural populations. In the first eight years of farm operation, approximately one thousand animals were used for sable breeding; some of these animals were able to adapt to the farm management and, subsequently, to the selection for a number of quantitative traits in the period of industrial domestication. It took about ten years for breeders to work out the breeding and selection technologies, which became successfully employed in the established affiliated sable breeding farms. The main achievement in sable breeding over the 85-year historical period of breeding in Russia is the creation of two unique breeds, black sable (1969) and Saltykovskaya 1 (2007). In general, industrial domestication in fur farming and the subsequent breeding works made the fur of many species (mink, fox, Arctic fox) obtained from natural populations uncompetitive, which undoubtedly reduced the hunting interest in the animals living in the wild. Consequently, hunting for fur-bearing animals of most species decreased and has only local importance. Owing to the specific features of sable biology, the fur of farm-bred animals cannot yet completely replace the furs obtained by hunting; however, the farm-bred sable population is constantly growing. This review presents the results of the analysis of the level of genetic variability in natural and farm populations at nuclear and mitochondrial loci. The comparative analysis makes it possible to estimate the loss of genetic diversity upon the species adaptation to the new conditions of existence.},
	keywords = {Animals; Female; Genetic Variation; Male; Mustelidae; Selective Breeding; animal; female; genetic variation; male; Mustelidae; physiology; selective breeding},
