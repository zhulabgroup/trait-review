@ARTICLE{Romero-Martínez2020,
	author = {Romero-Martínez, Ángel and Bressanutti, Sara and Moya-Albiol, Luis},
	title = {A systematic review of the effectiveness of non-invasive brain stimulation techniques to reduce violence proneness by interfering in anger and irritability},
	year = {2020},
