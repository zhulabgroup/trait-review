@ARTICLE{Cuezzo2021,
	author = {Cuezzo, Maria Gabriela and Medina, Regina Gabriela and Nieto, Carolina},
	title = {Geographic distribution modeling and taxonomy of Stephadiscus lyratus (Cothouny in Gould, 1846) (Charopidae) reveal potential distributional areas of the species along the Patagonian Forests},
