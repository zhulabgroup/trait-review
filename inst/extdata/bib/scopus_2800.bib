@ARTICLE{Zamudio-Beltrán2018,
	author = {Zamudio-Beltrán, Luz E. and Hernández-Baños, Blanca E.},
	title = {Genetic and morphometric divergence in the Garnet-Throated Hummingbird Lamprolaima rhami (Aves: Trochilidae)},
	year = {2018},
