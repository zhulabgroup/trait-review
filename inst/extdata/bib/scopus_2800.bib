@ARTICLE{Díaz-Castelazo2020,
	author = {Díaz-Castelazo, Cecilia and Martínez-Adriano, Cristian A. and Dáttilo, Wesley and Rico-Gray, Victor},
	title = {Relative contribution of ecological and biological attributes in the fine-grain structure of ant-plant networks},
	year = {2020},
	journal = {PeerJ},
