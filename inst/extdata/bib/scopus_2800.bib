@ARTICLE{Afsar2020,
	author = {Afsar, Sadia and Bibi, Gulnaz and Ahmad, Raza and Bilal, Muhammad and Naqvi, Tatheer Alam and Baig, Ayesha and Shah, Mohammad Maroof and Huang, Bangquan and Hussain, Jamshaid},
	title = {Evaluation of salt tolerance in Eruca sativa accessions based on morpho-physiological traits},
	year = {2020},
