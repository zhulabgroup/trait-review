	author = {Kontopoulos, Dimitrios-Georgios and García-Carreras, Bernardo and Sal, Sofía and Smith, Thomas P. and Pawar, Samraat},
	title = {Use and misuse of temperature normalisation in meta-analyses of thermal responses of biological traits},
	year = {2018},
